       function dynamicdropdown_student(listvalue)
         {
	     var standard;
             standard = "9th";
	     // var querystring = window.location.querystring;
	     // var myValue = querystring["useid"];
             document.getElementById("animptext").innerHTML ="";
             document.getElementById("imageplus").src = "/static/plan/all_student_pics/".concat(global_standard).concat("/").concat(listvalue).concat(".jpg");
         }         

 
           function dynamicdropdownone(listindex)
            {
                document.getElementById("goal1").length = 0;
                switch (listindex)
                
                {
                    case "Real Number System 7th" :
					   document.getElementById("goal1").options[0] = new Option("I  should know natural numbers and whole numbers.","I  should know natural numbers and whole numbers.");
					   document.getElementById("goal1").options[1] = new Option("I  should know stories addition,subtraction,multiplication and division of numbers.","I  should know stories addition,subtraction,multiplication and division of numbers.");
					   break;

                    case "Four Fundamental Operations on Integers" :
                    break;
                    case "Properties of Addition of integers" :
                    break;
                    case "Properties of subtraction of integers" :
                    break;
                    case "Properties of multiplication of integers" :
                    break;
                    case "Properties of division of integers" :
                    break;
					case "Fractions" :
                       document.getElementById("goal1").options[0] = new Option("I should know Proper,Improper and Mixed fraction.","I should know Proper,Improper and Mixed fraction.");
                       document.getElementById("goal1").options[1] = new Option("I should know stories for Addition,Subtraction,Multiplication and division of fractions.","I should know stories for Addition,Subrtaction,Multiplication and division of fractions.");
                    break;
                    case "Representation of Rational numbers on the Number line." :
                       document.getElementById("goal1").options[0] = new Option("I should know number line and be able to represent numbers in that.","I should know number line and be able to represent numbers in that.");
                    break;
                    case "Four Basic Operations on Rational numbers" :
			
                    break;
                    case "Decimal numbers" :
                    break;
                    case "Powers" :
                    break;
                    case "Algebraic Expressions" :
                       document.getElementById("goal1").options[0] = new Option("Stories for single variable expression.","Stories for single variable expression.");
                    break;
					case "Geometry 7th":
                    break;
					case "Practical Geometry 7th":
                    break;
                    case "Data Handling" :
                    break;
					case "Real Numbers 8th":
                        document.getElementById("goal1").options[0] = new Option("I should be able to do fraction Addition, Subtraction, Multiplication and Division.","I should be able to do fraction Addition, Subtraction, Multiplication and Division.");
                    break;
                    case "Measurements" :
                       document.getElementById("goal1").options[0] = new Option("I should be able to find the area and perimeter of a circle.","I should be able to find the area and perimeter of a circle.");
                    break;
					case "Geometry 8th":
                       document.getElementById("goal1").options[0] = new Option("I should be able to apply following properties to find an angle in a triangle","I should be able to apply following properties to find an angle in a triangle");
                       document.getElementById("goal1").options[1] = new Option("The sum of three angle is 180","The sum of three angle is 180");
                       document.getElementById("goal1").options[2] = new Option("Sum of adjacent angles of a straight line is 180","Sum of adjacent angles of a straight line is 180");
                       document.getElementById("goal1").options[3] = new Option("Any two sides of a triangle is greater than third angle","Any two sides of a triangle is greater than third angle");
                    break;
					case "Practical Geometry 8th":

                    break;
					case "Theory of Sets" :					
					break;
					case "Real Number System 9th" :
						document.getElementById("goal1").options[0] = new Option("I should be able to verify commutative, associative and distributive properties.","I should be able to verify commutative, associative and distributive properties.");
                       document.getElementById("goal1").options[1] = new Option("I should be able to solve Arithmetic expression using BODMAS.","I should be able to solve Arithmetic expression using BODMAS.");
                       document.getElementById("goal1").options[2] = new Option("I should be able to solve powers (example 23 = 2x2x2 = 8 or other way round).","I should be able to solve powers (example 23 = 2x2x2 = 8 or other way round).");
                       document.getElementById("goal1").options[3] = new Option("I should be able to solve Square and Square root of any number.","I should be able to solve Square and Square root of any number.");
                       document.getElementById("goal1").options[4] = new Option("I should be able to solve Cube and Cube root of any number.","I should be able to solve Cube and Cube root of any number.");
                    break;
					break;
					case "Algebra" :					
					break;
					case "Geometry 9th":
					document.getElementById("goal1").options[0] = new Option("I should be able to find out the hypotenuse or height or base of a Right angle triangle using Pythagoras theorem.","I should be able to find out the hypotenuse or height or base of a Right angle triangle using Pythagoras theorem.");
                    document.getElementById("goal1").options[1] = new Option("I should be able to apply following properties to find an angle in a triangle","I should be able to apply following properties to find an angle in a triangle");
                    document.getElementById("goal1").options[2] = new Option("Exterior angles, Interior angles, Corresponding angles, Alternative angles, Complementary angle, Supplementary angle","Exterior angles, Interior angles, Corresponding angles, Alternative angles, Complementary angle, Supplementary angle");
                    break;
					case "Co-ordinate Geometry" :
					break;
					case "Practical Geometry 9th":
					  document.getElementById("goal1").options[0] = new Option("I should be able to construct concentric circles when the radii are given and find out the width of each circular ring.","I should be able to construct concentric circles when the radii are given and find out the width of each circular ring.");
                     break;
                     default:
                        document.getElementById("goal1").options[0]=new Option("Please select the goal version","");
                        break;
               
                }				
