                      document.getElementById("goal3").options[10] = new Option("I should be able to find the area and perimeter of an irregular shapes or polygon.","I should be able to find the area and perimeter of an irregular shapes or polygon.");
                   break;
					case "Geometry 8th":
                      document.getElementById("goal3").options[0] = new Option("I should be able to apply following properties to find an angle in a triangle","I should be able to apply following properties to find an angle in a triangle");
                      document.getElementById("goal3").options[1] = new Option("Exterior angles","Exterior angles");
                      document.getElementById("goal3").options[2] = new Option("Interior angles","Interior angles");
                      document.getElementById("goal3").options[3] = new Option("Corresponding angles","Corresponding angles");
                      document.getElementById("goal3").options[4] = new Option("Alternative angles","Alternative angles");
                      document.getElementById("goal3").options[5] = new Option("Complementary angle","Complementary angle");
                      document.getElementById("goal3").options[6] = new Option("Supplementary angle","Supplementary angle");
                      document.getElementById("goal3").options[7] = new Option("I should be able to apply following conditions of Congruence to find an angle in a triangle","I should be able to apply following conditions of Congruence to find an angle in a triangle");
                      document.getElementById("goal3").options[8] = new Option("SSS Axiom","SSS Axiom");
                      document.getElementById("goal3").options[9] = new Option("SAS Axiom","SAS Axiom");
                      document.getElementById("goal3").options[10] = new Option("ASA Axiom","ASA Axiom");
                      document.getElementById("goal3").options[11] = new Option("S RHS Axiom","SRHS Axiom");
                   break;
					case "Practical Geometry 8th":
                      document.getElementById("goal3").options[0] = new Option("I should be able to construct quadrilateral based on the given measurements and also find its area.","I should be able to construct quadrilateral based on the given measurements and also find its area.");
                      document.getElementById("goal3").options[1] = new Option("I should be able to construct trapezium based on the given measurements and also find its area.","I should be able to construct trapezium based on the given measurements and also find its area.");
                      document.getElementById("goal3").options[2] = new Option("I should be able to construct parallelogram based on the given measurements and also find its area.","I should be able to construct parallelogram based on the given measurements and also find its area.");
                   break;
					case "Real Number System 9th":
                     document.getElementById("goal3").options[0] = new Option("I should be able to recall Natural numbers, Whole numbers, Integers.","I should be able to recall Natural numbers, Whole numbers, Integers.");
                     document.getElementById("goal3").options[1] = new Option("I should be able to classify rational numbers as recurring / terminating decimals.","I should be able to classify rational numbers as recurring / terminating decimals.");
                     document.getElementById("goal3").options[2] = new Option("I should be able to understand the existence of non-terminating and non recurring decimals.","I should be able to understand the existence of non-terminating and non recurring decimals.");
                     document.getElementById("goal3").options[3] = new Option("I should be able to represent terminating / non-terminating decimals on the number line.","I should be able to represent terminating / non-terminating decimals on the number line.");
                  break;
					case "Theory of Sets" :
                      document.getElementById("goal3").options[0] = new Option("I should be able to describe a set","I should be able to describe a set");
                      document.getElementById("goal3").options[1] = new Option("I should be able to represent sets in descriptive form, set builder form and roster form","I should be able to represent sets in descriptive form, set builder form and roster form");
                      document.getElementById("goal3").options[2] = new Option("I should be able to identify different kinds of sets","I should be able to identify different kinds of sets");
                      document.getElementById("goal3").options[3] = new Option("I should be able to understand and perform set operations","I should be able to understand and perform set operations");
                      document.getElementById("goal3").options[4] = new Option("I should be able to use Venn diagrams to represent sets and set operations","I should be able to use Venn diagrams to represent sets and set operations");
                      document.getElementById("goal3").options[5] = new Option("I should be able to use the formula involving n(AUB), n(A∩B) simple word problems","I should be able to use the formula involving n(AUB), n(A∩B) simple word problems");
                  break;
					case "Algebra" :
                     document.getElementById("goal3").options[0] = new Option("I should be able to classify polynomials and degrees.","I should be able to classify polynomials and degrees.");
                     document.getElementById("goal3").options[1] = new Option("I should be able to use Remainder Theorem.","I should be able to use Remainder Theorem.");
                     document.getElementById("goal3").options[2] = new Option("I should be able to use Factor Theorem.","I should be able to use Factor Theorem.");
                  break;
					case "Geometry 9th":
                     document.getElementById("goal3").options[0] = new Option("I should be able to recall the basic concepts of geometry.","I should be able to recall the basic concepts of geometry.");
                     document.getElementById("goal3").options[1] = new Option("I should be able to understand theorems on parallelograms.","I should be able to understand theorems on parallelograms.");
                  break;
                  case "Co-ordinate Geometry" :
                     document.getElementById("goal3").options[0] = new Option("I should be able to understand Cartesian coordinate system","I should be able to understand Cartesian coordinate system");
                     document.getElementById("goal3").options[1] = new Option("I should be able to identify abscissa, ordinate and coordinates of a point","I should be able to identify abscissa, ordinate and coordinates of a point");
                     document.getElementById("goal3").options[2] = new Option("I should be able to plot the points on the plane","I should be able to plot the points on the plane");
                     document.getElementById("goal3").options[3] = new Option("I should be able to find the distance between two points and apply it in various situations.","I should be able to find the distance between two points and apply it in various situations.");
                  break;
					case "Practical Geometry 9th":
					 document.getElementById("goal3").options[0] = new Option("I should be able to learn and construct perpendicular bisector and use it to learn Circumcentre and           Orthocentre.","I should be able to learn and construct perpendicular bisector and use it to learn Circumcentre and           Orthocentre.");
					 document.getElementById("goal3").options[1] = new Option("I should be able to learn and construct and learn about Circumcentre","I should be able to learn and construct and learn about Circumcentre");
					 document.getElementById("goal3").options[2] = new Option("I should be able to construct and learn about Orthocentre","I should be able to construct and learn about Orthocentre");
                  break;   
                    default:
                        document.getElementById("goal3").options[0] = new Option("Please select the goal version","");
                        break;
		
		}
                return true;
            }
       </script>
